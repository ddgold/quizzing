{
	"env": {
		"NODE_ENV": "development",
		"DATABASE_URL": "mongodb://localhost:27017",
		"ENGINE_CACHE_URL": "redis://localhost:6379",
		"JUDGE_URL": "http://localhost:9000",
		"SECRETS_DIR": "../../secrets",
		"SERVER_PORT": 8000
	},
	"exec": "FRONTEND_URL=\"http://${HOSTNAME:-localhost}:3000\" ts-node",
	"ext": "ts,json,graphql",
	"ignore": ["src/scripts"],
	"watch": "src/",
	"inspect": true
}
